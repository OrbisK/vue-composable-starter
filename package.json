{
  "name": "vue-composable-starter",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.12.3",
  "description": "ü™ù Minimalist starting point for your next Vue Composable",
  "author": "YaeÃàl GUILLOUX <yael.guilloux@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Tahul/vue-composable-starter#readme",
  "repository": "https://github.com/Tahul/vue-composable-starter",
  "bugs": {
    "url": "https://github.com/Tahul/vue-composable-starter/issues"
  },
  "keywords": [
    "vue",
    "composable"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist/**/*"
  ],
  "scripts": {
    "dev": "pnpm dev:vite",
    "build": "unbuild && pnpm build:nuxt",
    "prepare": "pnpm prepare:nuxt",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "pnpm run lint --fix",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test": "pnpm test:unit && pnpm test:coverage",
    "dev:nuxt": "pnpm --filter playgrounds-nuxt dev",
    "dev:vite": "pnpm --filter playgrounds-vite dev",
    "build:nuxt": "nuxi build playgrounds",
    "start:nuxt": "nuxi preview playgrounds",
    "prepare:nuxt": "pnpm --filter playgrounds-nuxt prepare:nuxt"
  },
  "dependencies": {
    "nuxt": "^3.14.159"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@types/fs-extra": "^11.0.4",
    "@vitest/coverage-v8": "^2.1.4",
    "c8": "^10.1.2",
    "eslint": "^9.14.0",
    "happy-dom": "^15.11.3",
    "lint-staged": "^15.2.10",
    "pascalcase": "^2.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.4",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
