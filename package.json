{
  "name": "vue-composable-starter",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.12.3",
  "description": "ü™ù Minimalist starting point for your next Vue Composable",
  "author": "YaeÃàl GUILLOUX <yael.guilloux@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Tahul/vue-composable-starter#readme",
  "repository": "https://github.com/Tahul/vue-composable-starter",
  "bugs": {
    "url": "https://github.com/Tahul/vue-composable-starter/issues"
  },
  "keywords": [
    "vue",
    "composable"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist/**/*"
  ],
  "scripts": {
    "dev": "vitest dev",
    "dev:demo": "vite",
    "build": "unbuild",
    "build:demo": "vite build",
    "lint": "eslint --flag unstable_ts_config .",
    "lint:fix": "pnpm run lint --fix",
    "test": "pnpm test:types && pnpm test:unit && pnpm test:coverage",
    "test:types": "tsc --build tsconfig.json",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "dev:nuxt": "nuxi dev playgrounds",
    "build:nuxt": "nuxi build playgrounds",
    "start:nuxt": "nuxi preview playgrounds"
  },
  "peerDependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest"
  },
  "dependencies": {
    "nuxt": "^3.14.159"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/test-utils": "^2.4.6",
    "c8": "^10.1.2",
    "eslint": "^9.14.0",
    "lint-staged": "^15.2.10",
    "pascalcase": "^2.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.4",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
